   "source": [
    "class Animal:\n",
    "    def sound(self):\n",
    "        print(\"generic sound\")\n",
    "class dog(Animal):\n",
    "    def sound(self):\n",
    "        print(\"bark\")\n",
    "class cat(Animal):\n",
    "    def sound(self):\n",
    "        print(\"meow\")\n",
    "\n",
    "dog=dog()\n",
    "cat=cat()\n",
    "#Overriding\n",
    "dog.sound()\n",
    "cat.sound()"
   ]
