   "source": [
    "### Write the program to find characters and digits in a string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "ab92ad16-19d1-4c44-bcc7-142f5c13a676",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter the string 1sdwe324\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Digits: 4\n",
      "alphabets 4\n"
     ]
    }
   ],
   "source": [
    "s=input(\"Enter the string\")\n",
    "dig=0\n",
    "alphabet=0\n",
    "for i in s:\n",
    "    if i.isdigit():\n",
    "        dig+=1\n",
    "    elif i.isalpha():\n",
    "        alphabet+=1\n",
    "\n",
    "print(\"Digits:\",dig)\n",
    "print(\"alphabets\",alphabet)\n",
    "\n",
    "    "
   ]
  },
